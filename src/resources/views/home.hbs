<div class="mt4">
    <div class="row">
        {{#each tests}}
        <div class="col-sm-6 col-lg-4">
            <div class="card" style="width: 18rem;">
                <div class="card-body">
                    <p class="card-text">Name : {{this.name}}</p>
                    <p class="card-text">State : {{this.state}}</p>
                    <p class="card-text">Power : {{this.power}}</p>
                    <p class="card-text">Command : {{this.command}}</p>

                </div>
            </div>
        </div>
        {{/each}}

    </div>
</div>